{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-string-throw": true,
        "no-unused-expression": [true],
        "member-access": true,
        "quotemark": [false],
        "semicolon": ["always"],
        "no-duplicate-imports": false,
        "object-literal-sort-keys": false,
        "ordered-imports": false,
        "no-console": false,
        "only-arrow-functions": false,
        "no-var-requires": false
    },
    "rulesDirectory": []
}